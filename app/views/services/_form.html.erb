<%= simple_form_for(@service) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :price %>
    <%= f.association :category %>
    <%= f.association :gender %>
    <%= f.association :level %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
